{
  "name": "curriculum",
  "version": "1.0.0",
  "description": "Proyecto Vue.JS",
  "main": "index.html",
  "scripts": {
    "dev": "parcel serve src/index.html --open",
    "watch": "parcel watch src/index.html --global global",
    "build": "rm -rf build/* && parcel build src/index.html -d build --global global --public-url /curriculum/",
    "clean:cache": "rm -rf .cache dist build",
    "clean:all": "rm -rf node_modules package-lock.json .cache dist build",
    "deploy": "git subtree push --prefix build origin gh-pages-axios"
  },
  "keywords": [
    "parcel",
    "template",
    "html5",
    "pug",
    "css3",
    "postcss",
    "stylelint",
    "typescript",
    "tslint",
    "vue",
    "javascript",
    "html",
    "eslint"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/creacodigos/curriculum.git"
  },
  "author": "Jorge Santana",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/creacodigos/curriculum/issues"
  },
  "homepage": "https://github.com/creacodigos/curriculum#readme",
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@vue/component-compiler-utils": "^2.6.0",
    "autoprefixer": "^9.5.1",
    "cssnano": "^4.1.10",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-html": "^5.0.3",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "npm-run-all": "^4.1.3",
    "parcel-plugin-bundle-visualiser": "^1.2.0",
    "postcss-clean": "^1.1.0",
    "postcss-font-magician": "^2.2.1",
    "postcss-mixins": "^6.2.1",
    "postcss-preset-env": "^6.6.0",
    "stylelint": "^9.10.1",
    "stylelint-config-standard": "^18.2.0",
    "vue-hot-reload-api": "^2.3.3",
    "vue-template-compiler": "^2.6.10"
  },
  "dependencies": {
    "assets": "^3.0.1",
    "axios": "^0.18.0",
    "vue": "^2.6.10"
  }
}
